{
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "installCommand": "npm install --legacy-peer-deps"
  }
  